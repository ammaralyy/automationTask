<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Shopping Regression Suite" parallel="tests" thread-count="2">
	<test name="windows Regression Test">
		<parameter name="browser" value="chrome"></parameter>
		<parameter name="platform" value="Windows 10"></parameter>
		<classes>
			<class name="tests.HomePageTest" />
		</classes>
	</test>
	<test name="mac Regression Test">
		<parameter name="browser" value="firefox"></parameter>
		<parameter name="platform" value="macOS 11.00"></parameter>
		<classes>
			<class name="tests.HomePageTest" />
		</classes>
	</test>
</suite>
